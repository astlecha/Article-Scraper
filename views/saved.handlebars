<div class="header">
  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
  <h5 class="modal-title">Comments</h5>
</div>
<div class="body">
  {{#each notes}}
    <form action="/note/{{this.id}}" method="GET">
    	<h4>{{this.body}}</h4>
    	<button type="button" class="btn btn-primary" data-dismiss="modal">Delete Note</button>
    </form>
</div>